class Solution(object):
    def eraseOverlapIntervals(self, intervals):
        """
        :type intervals: List[List[int]]
        :rtype: int
        """
        
        def get_end(item):
            return item[1]
        
        intervals.sort(key=get_end)
        
        
        
intervals = [[1,2],[2,3],[3,4],[1,3]]
solution=Solution()
print(solution.eraseOverlapIntervals(intervals))